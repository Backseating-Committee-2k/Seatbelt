function main() ~> Nothing {
    loop {
        break;
        continue;
    }

    let n: mutable U32 = 1;
    let k: mutable U32 = 0;
    loop {
        if n > 100 {
            break;
        }
        let n_plus_k: U32 = n + k;
        k = n;
        n = n_plus_k;
        continue;
        n = 10000;
    }
    std::assert_equals(n, 144);
    std::assert_equals(k, 89);
}
